<virtual-sensor name="test1" priority="10" >    <processing-class>		<class-name>org.openiot.gsn.vsensor.BridgeVirtualSensor</class-name>		<init-params/>	  <output-structure>	     <field name="field_1" type="bigint" />	     <field name="field_2" type="integer" />	     <field name="field_3" type="integer" />	     <field name="field_4" type="double" />	     <field name="field_5" type="char" />	     <field name="field_6" type="varchar" />	     <field name="field_7" type="binary:test/test" />      </output-structure>	</processing-class>    <description />    <life-cycle pool-size="10" />    <addressing>		<predicate key="latitude">1</predicate>	</addressing>    <storage history-size="10"/>    <streams/></virtual-sensor> 